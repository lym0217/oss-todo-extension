{
  "name": "ToDo Tab",
  "description": "Powerful new tab chrome extension - OSS Project",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}
